C     N1 : MAXIMUM NUMBER OF PRIMITIVE SCALAR BASIS FUNCTIONS (MOLFDIR)
C     N2 : MAXIMUM NUMBER OF PRIMITIVE SCALAR BASIS FUNCTIONS (HERMIT)
C     N22: MAXIMUM NUMBER OF CONTRACTED SCALAR BASIS FUNCTIONS
C     N28: MAXIMUM NUMBER OF SPINORS
C     N40: MAXIMUM NUMBER OF SPINORS IN ONE SUBREP
      PARAMETER (N1=25000)
      PARAMETER (N2=25000)
      PARAMETER (N22=3000)
      PARAMETER (N28=300)
      PARAMETER (N40=150)
      COMMON /VECS/ QA(N22,N28),QB(N22,N28),UA(N2,N28),UB(N2,N28)
      COMPLEX*16 QA,QB,UA,UB
      COMMON/VECOCC/ OCCMO(N28)
      COMMON /REORD/ IMORAS(N40,16),ITRMO(N28),NORB1,NORB2,NORB3
     &               ,KRA(N28,3),KRB(N28,3),IKRMO(N28)
      COMMON/KRASYM/RPHASE(N28)
      COMPLEX*16 RPHASE
      COMMON/SYMI/IMOREP(N28)
