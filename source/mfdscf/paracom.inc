      PARAMETER (N55=40000)
      PARAMETER (NNL=500)
      PARAMETER (NNS=1000)
      PARAMETER (NNT=NNS+NNL)
      PARAMETER (IDEF1=2,DEF2=1.0E-10,IDEF2=50)
