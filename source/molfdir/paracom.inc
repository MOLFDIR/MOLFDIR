C
C     DEFINITION PARAMETERS MOLFDIR:
C
C     N1  NUMBER OF NUCLEI
C     N2  NUMBER OF ELEMENTS IN THE PERIODIC SYSTEM STORED
C     N3  NUMBER OF SINGLE GAUSSIANS PER CONTRACTED FUNCTION
C     N5  NUMBER OF DIFFERENT L-VALUES FOR BASISFUNCTIONS
C     N6  NUMBER OF DIFFERENT L-VALUES TIMES MULTIPLICITY OF L
C     N7  SUM OVER UNIQUE NUCLEI TIMES NUMBER OF DIFF.FUNC. TYPES
C     N8  NUMBER OF COEFFICIENTS FOR AO'S IN SYMM.ADAPT.ORBITALS
C     N9  NUMBER OF SYMMETRY ADAPTED FUNCTIONS (.LT.2*N7)
C     N10 NUMBER OF COEFF. FOR GAUSSIANS IN SYMM.ADPT.ORBITALS
C     N12 NUMBER OF SYMMETRY FUNCTIONS
C     N14 NUMBER OF UNIQUE CONTRACTED GAUSSIANS
C     N15 NUMBER OF UNIQUE UNCONTRACTED GAUSSIANS (NUMBER OF EXPONENTS)
C     N16 NUMBER OF CONTRACTED GAUSSIANS OF THE SAME SYMMETRY
C     N17 NUMBER OF PRIMITIVE GAUSSIANS OF THE SAME SYMMETRY
C     NUMGRPS  NUMBER OF POINTGROUPS IMPLEMENTED IN XYZTRAN
C     (NUMBER MEANS MAXIMUM NUMBER)
C
      PARAMETER (N1=100)
      PARAMETER (N2=137)
C no proper check on N3 in mfd1nrm.f
      PARAMETER (N3=45)
      PARAMETER (N5=6,N6=N5*(N5+1)*(N5+2)/6)
      PARAMETER (N7=550)
      PARAMETER (N8=20000,N9=2*N7,N10=40000,N12=4000)
      PARAMETER (N14=700,N15=15500,N16=45,N17=45)
      PARAMETER (NUMGRPS=28)
C
C     DEFINITION PROGRAM COMMON BLOCKS
C
      COMMON/GEOMR/CHARGE(N1),RNUC(N1),XCORD(N1),YCORD(N1),ZCORD(N1),
     $             POTNUC
      COMMON/GEOMI/NEQNUC,NNUCL,NUCOP(N1,48),INDNUC(N1+1),LCHAIN,
     $             IDEFNUC
      COMMON/GEOC/CENTRN(N1),GROUPN(3),NREP(20),REPN(16),OPERN(48)
      COMMON/OEC/ALPHA(N15),COEFF(N15),XNORM(N15),COEFFH(N15)
      COMMON/CPN/NEQ(2),NGA(2),NCO(2),NEC(2),NEQSYMF,NSYMF,NCOEFF
      COMMON/TYP/NTYPE(N5),KTYPE(N5+1),MINTYP,MAXTYP
      COMMON/NUCW/RNWGHT (N2), INWGHT (N2)
      COMMON/CHAR/NAME,TYPE(N5),TYPES(0:N5),ATOMS(0:N2),FN,FNAME(84),
     $            VERS,AUANG,BASFLS(2,N1)
      COMMON/CON/PI,FFACT(N5),BOHRRAD,BOHRFM
      COMMON/NLM/NX(N6),NY(N6),NZ(N6)
      COMMON/CTY/ICENT(N7),ITYPE(N7),INDCOM(N7+1),ICOMOP(N7,48),
     $           NSYMC(N7),INDSC(N7+1),NSUBRP,IREPS(16)
      COMMON/SBD/NORBIT(N14),KCOMB(N14),INDBAS(N14+1),
     $           INDSYM(N14+1),INDCPRI(N14),INDCCON(N14)
      COMMON/SCRR/CSYMORA(N8),CSYMORB(N8),CTRANA(N10),CTRANB(N10)
      COMMON/SCRI/XM1(80),NOPER(3),KDIMRP(10),ISYGEN(0:NUMGRPS),
     $            INDCHAR(NUMGRPS),NUMCLS(0:NUMGRPS),
     $            INDCLAS(NUMGRPS),ICCLAS(45),INDCH1(48),INDCH2(24),
     $            INDCH3(12),INVOP1(48),INVOP2(48),IDFOPER(3,48),
     $            ICHAIN(14:28),INVERS(24),INDREP(NUMGRPS),
     $            JNDREP(13),JNDCHAR(13)
      COMMON/SCRR1/CHARACT(387),U(192),CF(48),CHARACS(130)
      COMMON/SCRI1/INDSOR(N9+1),IRPSOR(N9),
     $             INDYC(0:24),LSYMC(24),LBASC(24),INDTYP(N6),
     $             ITRAN(N10),INDORB(N12+1),KSYMRP(N12),LSYMRP(N12),
     $             KSYMF(N12),INDB(N12),NBSYM(16),NBSYMX(16,2)
      COMMON/OPDAT/GENER(3,NUMGRPS),OPERATI(48,3), OPSIGN (24, 24),
     $             OPERAT1(48),OPERAT2(24),OPERAT3(12),OPTABEL(48,48)
      COMMON/CHDATA/GRNAME(NUMGRPS),GREP(66),OPNAME(48),GREP1(32)
      COMMON/GROUPI/NSYMOP,XMAT(N6,48),NSYMRP,IDIMRP(10),
     $              IGRID(3),NSUBSP,IFASOP(2),IFIOP
      COMMON/SYMR/YLMC(136),ZLMC(136)
      COMMON/SYML/FRLCC,FRLC(3)
      COMMON/SYMI/IFN(85)
      COMMON/SYS/PRNT
      COMMON/SYSL/TWOC,AREP,RNDHER,PROPERTY
      COMMON/INTI/ICTA,ICTB,ICENTA,ITYPA,ICENTB,ITYPB
      COMMON/INTR/SINT
C
      INTEGER PRNT
      INTEGER XM1,XMAT
      INTEGER OPERATI,OPERAT1,OPERAT2,OPERAT3,OPSIGN,OPTABEL,GENER
C
      LOGICAL FRLCC,FRLC,TWOC,AREP,RNDHER,PROPERTY
C
      CHARACTER*4 CENTRN*2,GROUPN*3,NREP,REPN*14,OPERN
      CHARACTER*1 NAME*80,TYPE,TYPES,ATOMS*2,FN*32,FNAME*6,VERS*10,
     $            AUANG*3,BASFLS*80
      CHARACTER*4 GRNAME*3,GREP,OPNAME,GREP1
C
      COMPLEX*16 CF,CHARACT,CHARACS
      COMPLEX*16 CSYMORA,CSYMORB
      COMPLEX*16 CTRANA,CTRANB
